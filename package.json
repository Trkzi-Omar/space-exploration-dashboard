{
  "name": "space-exploration-dashboard",
  "version": "1.0.0",
  "description": "Dashboard using Nasa API",
  "main": "index.js",
  "scripts": {
    "build": "pnpm build:backend && pnpm build:frontend",
    "build:backend": "cd backend && pnpm run build",
    "build:frontend": "cd frontend && pnpm run build",
    "dev": "pnpm dev:backend & pnpm dev:frontend",
    "dev:backend": "cd backend && pnpm dev",
    "dev:frontend": "cd frontend && pnpm dev",
    "postinstall": "pnpm --filter backend install && pnpm --filter frontend install"
  },
